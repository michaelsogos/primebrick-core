{
    "$schema": "../../../../schemas/data-init.schema.json",
    "version": "1.0.0",
    "defs": [
        {
            "name": "translations-importer",
            "files": ["translations-it.csv", "translations-en.csv"],
            "type": "csv",
            "csvOptions": {
                "entity": "MetaTranslation",
                "encoding": "utf8",
                "checkColumns": ["key", "languageCode"]
            }
        },
        {
            "name": "menu-importer",
            "files": ["menu-item.csv"],
            "type": "csv",
            "csvOptions": {
                "entity": "MetaMenuItem",
                "encoding": "utf8",
                "checkColumns": ["importId"],
                "circularRelation": {
                    "parentColumn": "$parent",
                    "mappedByColumn": "importId",
                    "childKeyColumn": "importId",
                    "parentField": "parent"
                },
                "cardinalRelations": [
                    {
                        "cardinality": "many-to-many",
                        "targetEntity": "Role",
                        "targetKeyColumn": "$role",
                        "targetKeyField": "name",
                        "sourceKeyColumn": "importId",
                        "sourceRelationField": "roles"
                    }
                ]
            }
        }
    ]
}
