{
    "name": "primebrick-core",
    "version": "0.1.0",
    "description": "Primebrick Core",
    "main": "dist/main.js",
    "author": "Michael Sogos",
    "license": "MIT",
    "engines": {
        "node": ">=14.12.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/michaelsogos/primebrick-core.git"
    },
    "bugs": {
        "url": "https://github.com/michaelsogos/primebrick-core/issues",
        "email": "michael.sogos@gurustudioweb.it"
    },
    "homepage": "https://github.com/michaelsogos/primebrick-core#readme",
    "files": [
        "package.json",
        "LICENSE",
        "dist/**"
    ],
    "keywords": [
        "primebrick",
        "api"
    ],
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,json}\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "migration:create": "typeorm migration:generate -n core-entities",
        "migration:run": "typeorm migration:run",
        "update:sdk": "yarn add -E primebrick-sdk",
        "update:core-commons": "yarn add -E primebrick-commons-core"
    },
    "dependencies": {
        "@nestjs/common": "8.0.4",
        "@nestjs/core": "8.0.4",
        "@nestjs/microservices": "8.0.4",
        "@nestjs/platform-express": "8.0.4",
        "@nestjs/schedule": "1.0.0",
        "@nestjs/typeorm": "8.0.1",
        "pg": "8.6.0",
        "primebrick-sdk": "0.177.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.2.0",
        "typeorm": "0.2.34"
    },
    "devDependencies": {
        "@nestjs/cli": "8.0.2",
        "@nestjs/schematics": "8.0.2",
        "@nestjs/testing": "8.0.4",
        "@types/cron": "1.7.3",
        "@types/express": "4.17.13",
        "@types/jest": "26.0.24",
        "@types/node": "16.3.3",
        "@types/supertest": "2.0.11",
        "@typescript-eslint/eslint-plugin": "4.28.3",
        "@typescript-eslint/parser": "4.28.3",
        "eslint": "7.31.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.23.4",
        "jest": "27.0.6",
        "prettier": "2.3.2",
        "supertest": "6.1.3",
        "ts-jest": "27.0.3",
        "ts-loader": "9.2.3",
        "ts-node": "10.1.0",
        "tsconfig-paths": "3.10.1",
        "typescript": "4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            ".+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
